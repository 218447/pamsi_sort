#include "QuickSort.hh"

QuickSort::quickSort() {
  listOfElements=new List;
  lessThanX=new List;
  moreOfElements=new List;
}
QuickSort::~quickSort() {
  delete listOfElements;
  delete lessThanX;
  delete moreOfElements;
}
int chooseX() {
  srand (time(NULL));
  int a = rand() % listOfElements.size();    
  int b = rand() % listOfElements.size();
  X=a+b/2;
}
